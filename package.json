{
  "name": "stunk",
  "version": "2.2.3",
  "description": "Stunk is a lightweight, framework-agnostic state management library for JavaScript and TypeScript. It uses chunk-based state units for efficient updates, reactivity, and performance optimization in React, Vue, Svelte, and Vanilla JS/TS applications.",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsup",
    "build:types": "tsc --project tsconfig.json",
    "prepublishOnly": "npm run build && npm test",
    "test": "vitest",
    "lint": "eslint . --ext .js,.ts,.tsx,.vue"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/I-am-abdulazeez/stunk"
  },
  "homepage": "https://stunk.vercel.app/",
  "bugs": {
    "url": "https://github.com/I-am-abdulazeez/stunk/issues"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./middleware": {
      "types": "./dist/types/middleware/index.d.ts",
      "import": "./dist/middleware/index.js",
      "require": "./dist/middleware/index.cjs"
    },
    "./react": {
      "types": "./dist/types/use-react/index.d.ts",
      "import": "./dist/use-react/index.js",
      "require": "./dist/use-react/index.cjs"
    }
  },
  "keywords": [
    "state-management",
    "atomic-state",
    "chunk-based state",
    "framework-agnostic",
    "reactive state library",
    "frontend state management",
    "JavaScript state management",
    "TypeScript state management",
    "React state management",
    "Vue state management",
    "Svelte state management",
    "recoil alternative",
    "jotai alternative",
    "zustand alternative",
    "lightweight state management",
    "state container",
    "reusable state",
    "efficient state updates",
    "performance optimization",
    "stunk",
    "chunk"
  ],
  "author": "AbdulAzeez",
  "contributors": [
    {
      "name": "AbdulAzeez",
      "url": "https://github.com/I-am-abdulazeez"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0 || ^17.0.0 || ^18.0.0",
    "@types/react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^4.6.0 || ^5.2.0",
    "jsdom": "^26.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.8"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
